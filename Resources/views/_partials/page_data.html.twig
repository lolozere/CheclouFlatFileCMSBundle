{%- else -%}
{{ block('checlou_flat_file_cms_data_page_title') }}
{%- endif -%}
{%- endblock -%}


{%- block checlou_flat_file_cms_data_page_meta_description -%}
{{ page.headers.meta.description|default(block('checlou_flat_file_cms_data_page_summary')) }}
