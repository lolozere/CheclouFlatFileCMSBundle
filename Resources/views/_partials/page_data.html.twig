{%- block checlou_flat_file_cms_data_page_meta_keywords -%}
{{ page.headers.meta.keywords|default("") }}
{%- endblock -%}

{%- block checlou_flat_file_cms_data_page_summary -%}
{{ checlou_flat_file_cms_page_summary(page) }}
{%- endblock -%}

