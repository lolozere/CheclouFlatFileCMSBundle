{
  "name": "checlou/flat-file-cms-bundle",
  "type": "symfony-bundle",
  "description": "Bundle to add a CMS based on markdown files to an symfony web app",
  "authors": [
    {
      "name": "Laurent Chedanne",
      "email": "git@chedanne.pro"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2.5",
    "ext-mbstring": "*",
    "erusev/parsedown": "^1.7",
    "erusev/parsedown-extra": "^0.8.1",
    "symfony/finder": "^5.4|^6.0",
    "symfony/yaml": "^5.4|^6.0",
    "symfony/routing": "^5.4|^6.0",
    "symfony/config": "^5.4|^6.0",
    "symfony/asset": "^5.4|^6.0",
    "judev/php-htmltruncator": "^1.2",
    "twig/twig": "^2.12|^3.0",
    "symfony/twig-bridge": "^5.4|^6.0",
    "twig/intl-extra": "^3.7",
    "symfony/framework-bundle": "^5.4|^6.0",
    "symfony/twig-bundle": "^5.4|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "symfony/http-kernel": "^5.4|^6.0",
    "symfony/browser-kit": "^5.4|^6.0",
    "symfony/web-profiler-bundle": "^5.4|^6.0",
    "symfony/css-selector": "^5.4|^6.0"
  },
  "autoload": {
    "psr-4": { "Checlou\\FlatFileCMSBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": { "Checlou\\FlatFileCMSBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "minimum-stability": "stable"
}